{
  "name": "frourio-app-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:server": "webpack --watch --mode=development",
    "dev:frourio": "frourio --watch",
    "dev:prisma": "prisma generate --watch",
    "build": "npm run build:frourio && webpack --mode=production",
    "build:frourio": "npm run migrate:dev && prisma generate && frourio",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "start": "cross-env NODE_ENV=production node index.js"
  },
  "dependencies": {
    "@prisma/client": "2.18.0",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "fastify": "^3.14.0",
    "fastify-cors": "^5.2.0",
    "fastify-helmet": "^5.3.0",
    "fastify-jwt": "^2.3.0",
    "fastify-multipart": "^4.0.2",
    "fastify-static": "^4.0.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.18.0",
    "@types/busboy": "^0.2.3",
    "@types/jest": "^26.0.20",
    "cross-env": "^7.0.3",
    "frourio": "^0.24.1",
    "nodemon-webpack-plugin": "^4.4.4",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.4.1",
    "typescript": "^4.2.3",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  }
}
